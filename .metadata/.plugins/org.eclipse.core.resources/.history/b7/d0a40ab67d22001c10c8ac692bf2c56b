package practice_day_5;

import java.util.Scanner;

public class Main4 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] arr = new int[10];
		int[] count = new int[10];
		
		for(int i = 0; i<arr.length;i++) {
			arr[i] = sc.nextInt();
		}
		
		for(int i = 0; i< arr.length; i++) {
			if(arr[i]) {
				count[i]++;
			}
				
			
		}
		System.out.println(count[]);	
	}
}
