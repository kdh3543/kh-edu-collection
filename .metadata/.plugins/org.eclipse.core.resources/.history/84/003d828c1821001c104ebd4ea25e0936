package practice_day_5;

import java.util.Scanner;

public class Num3_Multple {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		int num3 = sc.nextInt();

		int[] arr = new int[10];
		int[] count = new int[10];
		for(int i=0;i<arr.length;i++) {
			arr[i] = i;
		}

		int sum = num1 * num2 * num3;
		int sum1 = sum%10;
		for(int i = 0;i<sum;i++) {			
			int sum2 = (sum/10)%10;
			int sum3 = sum/10;
			sum = sum3;
			for(int j = 0; j< arr.length;j++) {
				if(sum1 == arr[j]) {
					count[j]++;
				}
				if(sum2 == arr[j]) {
					count[j]++;
				}
			}
		}
		for(int i = 0; i<arr.length;i++) {
			System.out.println(i+ "ÀÌ ³ª¿Â È½¼ö: " + count[i]);
		}
	}
}
