package practice_day_4;

import java.util.Scanner;

public class Num2_While2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int num = sc.nextInt();
		if(num<0||num>99) {
			System.out.println("잘못된 입력입니다.");
		}
		int fst = num%10;
		System.out.println(fst);
		

	}

}
